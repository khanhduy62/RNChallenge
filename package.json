{
    "name": "RNChallenge",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "cd android &&./gradlew clean && cd.. && npx react-native run-android ",
        "ios": "react-native run-ios",
        "rl-android":"yarn apk && yarn update-android",
        "apk": "cd android &&./gradlew clean && ./gradlew assembleRelease && cd .. ",
